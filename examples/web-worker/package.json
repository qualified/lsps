{
  "name": "@qualified/codemirror-workspace-demo-worker",
  "version": "0.1.13",
  "private": true,
  "scripts": {
    "dev": "pnpm build-worker && vite",
    "build-worker": "rollup -f es -i src/worker.ts -p typescript -p 'node-resolve={browser: true}' -p commonjs -o public/worker.js",
    "build": "tsc && pnpm build-worker && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@qualified/codemirror-workspace": "workspace:0.4.2",
    "codemirror": "^5.59.2",
    "marked": "^4.0.17",
    "vscode-json-languageservice": "^5.0.0",
    "vscode-languageserver-protocol": "^3.16.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/codemirror": "^5.0.0",
    "@types/marked": "^4.0.3",
    "rollup": "^2.75.7",
    "typescript": "^4.7.4",
    "vite": "^2.9.9"
  }
}
