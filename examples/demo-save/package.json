{
  "name": "@qualified/codemirror-workspace-demo-save",
  "version": "0.1.12",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm start-ls-rust\" \"vite\"",
    "build": "tsc && vite build",
    "preview": "concurrently \"pnpm start-ls-rust\" \"vite preview\"",
    "start-ls-rust": "cd ./workspace && PATH=$(pwd)/../bin:$PATH lsp-ws-proxy -l 9999 --remap --sync -- rust-analyzer"
  },
  "dependencies": {
    "@qualified/codemirror-workspace": "workspace:0.4.1",
    "codemirror": "^5.59.2",
    "marked": "^4.0.17"
  },
  "devDependencies": {
    "@types/codemirror": "^5.0.0",
    "@types/marked": "^4.0.3",
    "concurrently": "^5.3.0",
    "typescript": "^4.0.2",
    "vite": "^2.9.9"
  }
}
