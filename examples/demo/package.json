{
  "name": "@qualified/codemirror-workspace-demo",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "build": "rimraf public/js && webpack",
    "start-proxy": "PATH=$(pwd)/bin:$PATH lsp-ws-proxy -l 9990 -- typescript-language-server --stdio -- css-languageserver --stdio -- html-languageserver --stdio",
    "start": "pnpm build && concurrently \"pnpm start-proxy\" \"serve -l 4000 public\""
  },
  "dependencies": {
    "@qualified/codemirror-workspace": "workspace:0.2.3",
    "codemirror": "^5.56.0",
    "marked": "^2.0.7",
    "typescript-language-server": "^0.4.0",
    "vscode-css-languageserver-bin": "^1.4.0",
    "vscode-html-languageserver-bin": "^1.4.0"
  },
  "devDependencies": {
    "@types/codemirror": "^5.0.0",
    "@types/marked": "^2.0.3",
    "@types/webpack": "^4.41.22",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  }
}
